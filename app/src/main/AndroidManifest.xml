<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.paincker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".widget.observablescrollview.ObservableScrollViewActivity" />
        <activity android:name=".widget.roundcornerlayout.RoundCornerLayoutActivity" />
        <activity android:name=".widget.labelview.LabelViewActivity" />
        <activity android:name=".develop.DevelopPanelActivity" />
        <activity android:name=".widget.scrollvelocity.demo.ScrollVelocityActivity"/>
        <activity android:name=".widget.scrollvelocity.demo.RecyclerVelocityActivity"/>
        <activity android:name=".widget.scrollvelocity.demo.ListVelocityActivity"/>
        <activity android:name=".widget.scrollvelocity.demo.ScrollViewVelocityActivity"/>
    </application>

</manifest>